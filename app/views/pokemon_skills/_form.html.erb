<%= form_with(model: pokemon_skill, local: true) do |form| %>
  <div class="form-group col-md-12">
    <%= form.label :skill_id %>
    <%= select_tag 'pokemon_skill[skill_id]', options_for_select(@skill_select), :class=>"form-control" %>
  </div>
  <div class="form-group col-md-9">
    <%= form.hidden_field :pokemon_id, value: pokemon.id %>
  </div>
  <div class="form-group col-md-9">
    <%= form.submit :class=>"btn btn-primary" %>
  </div>
<% end %>